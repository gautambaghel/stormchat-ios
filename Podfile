target 'stormchat' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for stormchat
  pod 'SlackTextViewController'
  pod 'SnapKit'

  target 'stormchatTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'stormchatUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
